{
  "name": "openapi-cop-mock-server",
  "version": "1.0.0",
  "description": "Mock server for use together with the openapi-cop proxy server",
  "license": "MIT",
  "author": "Alexis Luengas",
  "private": true,
  "main": "./build/src/app",
  "bin": "./build/src/cli.js",
  "scripts": {
    "postinstall": "npm run compile",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "start": "node build/src/cli",
    "dev-start": "npm run compile && node build/src/cli --port 9000 --file",
    "pretest": "npm run compile",
    "test": "jest --forceExit --detectOpenHandles --maxWorkers=10",
    "test:deps": "depcheck",
    "posttest": "npm run check"
  },
  "dependencies": {
    "chalk": "4.1.2",
    "chokidar": "3.5.3",
    "commander": "4.1.1",
    "debug": "4.3.3",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "json-schema-ref-parser": "9.0.9",
    "js-yaml": "3.14.1",
    "morgan": "1.10.0",
    "openapi-backend": "3.9.2",
    "source-map-support": "0.5.21",
    "which": "2.0.2"
  },
  "devDependencies": {
    "@types/errorhandler": "1.5.0",
    "@types/jest": "27.4.0",
    "@types/js-yaml": "3.12.7",
    "@types/json-schema": "7.0.9",
    "@types/morgan": "1.9.3",
    "@types/node": "11.15.53",
    "@types/prettier": "2.6.0",
    "@types/swagger-parser": "4.0.3",
    "@types/wait-on": "5.3.1",
    "@types/which": "2.0.1",
    "axios": "0.19.2",
    "depcheck": "1.4.3",
    "gts": "3.1.0",
    "jest": "27.5.1",
    "ts-jest": "27.1.3",
    "tslint": "6.1.3",
    "typescript": "4.0.5",
    "wait-on": "6.0.1"
  }
}
